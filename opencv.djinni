@extern "opencv.yaml"

opencv = interface +c {
  static create(): opencv;
  get_canny(path: string): string;
  get_threshold(path: string): string;
  get_lines(path: string): string;
  get_contours(path: string): string;
  get_segments(path: string): string;
  get_round_contours(path: string): string;
}
